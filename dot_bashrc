#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

eval "`dircolors -b ~/.dircolors`"
alias ls='ls --color=auto'
export EDITOR='nvim'

alias chz=chezmoi
alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias ll='ls -laF --color=auto'
PS1='[\u@\h \W]\$ '

